<%= simple_form_for(@empleado) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nombre %>
    <%= f.input :apellidos %>
    <%= f.input :identificacion %>
    <%= f.association :cargo %>
    <%= f.input :mail %>
    <%= f.input :tel %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
